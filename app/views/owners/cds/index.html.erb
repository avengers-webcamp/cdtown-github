<div class="container clearfix">
 <div class="user_cd_index">
   <div class="row">
    <div class="owner_cds_index">
     <%=link_to '新規登録', new_owners_cd_path, class:"btn-lg btn-primary " %>
    </div>
   </div>

          <% @cds.each do |cd| %>

          <div class="cd">
              <p><span><%= attachment_image_tag cd, :jacket_image, :fill, 180, 180, format: 'jpeg' %></span></p>
            <div class="user_cd_index_cdname">
              <p><span><%= cd.name %></span></p>
            </div>
            <div class="user_cd_index_cdprice">
              <p><span><%= cd.price %></span></p>
            </div>
              <p><span><%= cd.stock %></span></p>
            <div class="cart_show">
              <p><span><%= link_to '詳細', owners_cd_path(cd.id),class:"btn-sm btn-light btn-lg" %></span></p>
            </div>
          </div>

          <% end %>
 </div>
</div>
