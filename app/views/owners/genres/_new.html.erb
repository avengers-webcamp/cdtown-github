<div class="container">
 <div class="row">
  <div class="col-md-8">

  	<%= form_for @genre, url: owners_genres_path, method: :post do |f| %>
    <%= render 'layouts/error_messages', model: f.object %>
  	<div class="row">
  	  <table>
  	  	<tr>
  	  	  <th>ジャンル</th>
  	  	</tr>
  	  	<tr>
  	  	  <th><%= f.text_field :name %></th>
  	  	</tr>
  	  </table>
  	</div>


  	<div class="row">
     <div class="create_form_submit">
      <%= f.submit "登録", class:"btn btn-primary" %>
     </div>
    </div>

    <% end %>

  </div>
 </div>
</div>